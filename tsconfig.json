{
    "compilerOptions": {
        "module": "commonjs",
        "target": "es6",
        "noImplicitAny": true,
        "moduleResolution": "node",
        "sourceMap": true,
        "outDir": "dist",
        "baseUrl": ".",
        "paths": {
            "*": [
                "node_modules/*",
                "d/*"
            ]
        },
        "removeComments": true,
        "preserveConstEnums": true
    },
    "files": [
        "lib.ts"
    ]
}